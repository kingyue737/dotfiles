{
  "auths": {},
  "credsStore": "desktop",
  "currentContext": "desktop-linux",
  "plugins": {
    "debug": {
      "hooks": "exec"
    },
    "scout": {
      "hooks": "pull,buildx build"
    }
  },
  "features": {
    "hooks": "true"
  },
  "proxies": {
    "default": {
      "noProxy": "*.test,localhost,127.0.0.0/8",
      "httpProxy": "http://host.docker.internal:7890",
      "httpsProxy": "http://host.docker.internal:7890"
    }
  }
}
